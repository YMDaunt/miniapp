/* pages/rank/rank.wxss */

.container {
    padding: 0 30rpx;
}

.nav-list {
    height: 130rpx;
    line-height: 130rpx;
    color: #fff;
    font-size: 37rpx;
}

.rank-list {
    max-height: 785rpx;
}

.item {
    position: relative;
    height: 132rpx;
    background-color: #5d098c;
    border-radius: 12rpx;
}

.item .index {
    position: relative;
    z-index: 1;
    float: left;
    width: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    line-height: 132rpx;
}

.item .index1 {
    position: absolute;
    top: 40rpx;
    left: 20rpx;
    width: 49rpx;
    height: 65rpx;
    z-index: 0;
    background: url('//static.guojiang.tv/src/miniapp/guessSongs/index/index1.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.item .index2 {
    position: absolute;
    top: 40rpx;
    left: 20rpx;
    width: 49rpx;
    height: 65rpx;
    z-index: 0;
    background: url('//static.guojiang.tv/src/miniapp/guessSongs/index/index2.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.item .index3 {
    position: absolute;
    top: 40rpx;
    left: 20rpx;
    width: 49rpx;
    height: 65rpx;
    z-index: 0;
    background: url('//static.guojiang.tv/src/miniapp/guessSongs/index/index3.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.item .avatar-box, .my-list .avatar-box {
    float: left;
    width: 83rpx;
    height: 83rpx;
    margin: 25rpx;
    border-radius: 50%;
    overflow: hidden;
}

image {
    width: 100%;
    height: 100%;
}

.item .info, .my-list .info {
    display: flex;
    align-items: center;
    float: left;
    height: 132rpx;
}

.item .info .name, .my-list .info .name {
    font-size: 34rpx;
    color: #fff;
}

.item .info .coin, .my-list .info .coin {
    font-size: 30rpx;
    color: #ffdd48;
}

.item .info .coin::after, .my-list .info .coin::after {
    content: "";
    display: block;
    float: left;
    margin: 12rpx 10rpx 0 0;
    width: 22rpx;
    height: 24rpx;
    background-image: url('data:image/png;base64,');
    background-size: cover;
}

.item .songNum, .my-list .songNum {
    float: right;
    color: #fff;
    font-size: 34rpx;
    line-height: 132rpx;
    margin-right: 50rpx;
}

.my-list {
    height: 132rpx;
    background-color: #410065;
    border-radius: 12rpx;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
}

.my-list .notlist {
    line-height: 132rpx;
}

.my-list .index {
    float: left;
    width: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    line-height: 132rpx;
}
