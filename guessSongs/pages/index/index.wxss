/**index.wxss**/

.navigator-hover {
    opacity: 0.8;
}

.logo {
    margin-top: 134rpx;
    width: 100%;
    height: 599rpx;
    background: url('https://static.guojiang.tv/src/miniapp/guessSongs/index/logo.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.userInfo {
    position: absolute;
    left: 35rpx;
    top: 27rpx;
    height: 84rpx;
    width: 270rpx;
    border-radius: 42rpx;
    background-color: rgba(0, 0, 0, 0.3);
}

.userInfo .avatar {
    float: left;
    width: 83rpx;
    height: 83rpx;
    border-radius: 50%;
    border: 1rpx solid #ffdd48;
    overflow: hidden;
}

.userInfo image {
    width: 100%;
    height: 100%;
}

.userInfo .info {
    float: left;
    margin-left: 20rpx;
}

.userInfo .info .nickname {
    color: #fff;
    font-size: 32rpx;
}

.userInfo .info .coin {
    color: #ffdd48;
    font-size: 26rpx;
}

.userInfo .coin::before {
    content: "";
    display: block;
    float: left;
    margin: 6rpx 10rpx 0 0;
    width: 22rpx;
    height: 24rpx;
    background-image: url('data:image/png;base64,');
    background-size: cover;
}

.userInfo .coin::after {
    content: "";
    float: right;
    width: 9rpx;
    height: 18rpx;
    margin: 12rpx 0 0 12rpx;
    background-image: url('data:image/png;base64,');
    background-size: cover;
}

.login-btn {
    margin-top: 20rpx;
    width: 390rpx;
    height: 153rpx;
    background: url('data:image/png;base64,') no-repeat 0 0 scroll;
    background-size: cover;
}

.begin-btn {
    width: 390rpx;
    height: 153rpx;
    margin: 0 auto;
    background: url('https://static.guojiang.tv/src/miniapp/guessSongs/index/begin-btn.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.pk-btn {
    margin: 30rpx 35rpx 0 0;
    float: right;
    width: 340rpx;
    height: 191rpx;
    background: url('https://static.guojiang.tv/src/miniapp/guessSongs/index/pk-btn.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.rank-btn {
    margin: 30rpx 0 0 35rpx;
    float: left;
    width: 340rpx;
    height: 191rpx;
    background: url('https://static.guojiang.tv/src/miniapp/guessSongs/index/rank-btn.png') no-repeat 0 0 scroll;
    background-size: cover;
}

.task {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.5);
}

.task .task-content-box {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.task .close {
    position: absolute;
    top: -50rpx;
    right: 10rpx;
    width: 28rpx;
    height: 28rpx;
    background-image: url('data:image/png;base64,');
    background-size: cover;
}

.task .task-item {
    position: relative;
    width: 600rpx;
    height: 136rpx;
    background-color: #5d098c;
    border-radius: 12rpx;
}

.task .hasLimitTask {
    padding-top: 25rpx;
    height: 140rpx;
    overflow: unset;
    background-color: #7801ba;
}

.task-item .item-content {
    float: left;
    margin: 30rpx 0 30rpx 30rpx;
}

.item-content .title {
    font-size: 32rpx;
    color: #fff;
}

.item-content .prize {
    font-size: 30rpx;
    color: #ffdd48;
}

.item-content .prize::before {
    content: "";
    display: block;
    float: left;
    width: 22rpx;
    height: 24rpx;
    margin: 10rpx 5rpx 0 0;
    background-image: url('data:image/png;base64,');
    background-size: cover;
}

.task .task-btn {
    float: right;
    color: #fff;
    width: 150rpx;
    height: 76rpx;
    background: url('https://static.guojiang.tv/src/miniapp/guessSongs/index/task-btn.png');
    background-size: cover;
    font-size: 32rpx;
    text-align: center;
    line-height: 76rpx;
    margin: 30rpx;
}

.limit-task {
    position: absolute;
    top: 10rpx;
    left: -10rpx;
    font-size: 28rpx;
    color: #5d098c;
    padding: 5rpx 10rpx;
    background-color: #fff;
    border-radius: 12rpx;
}
